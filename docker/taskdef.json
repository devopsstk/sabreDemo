{
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "volumesFrom": [],
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081,
          "protocol": "tcp"
        }
      ],
	    "logConfiguration": {
	        "logDriver": "awslogs",
	        "options": {
	            "awslogs-group": "demoapp-dev",
	            "awslogs-region": "us-west-1",
	            "awslogs-stream-prefix": "demoapp-dev"
	        }
	    },
      "essential": true,
      "mountPoints": [],
      "name": "demoapp",
      "environment": [{
		  "name" : "env",
		  "value" : "dev" 
		}],
      "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
      "cpu": 0
    }
  ],
  "placementConstraints": [],
  "volumes": [],
  "family": "demoapp-dev"
}
